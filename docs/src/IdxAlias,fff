\IA{learned sequences}{learning}
\IA{line edit}{line editing}
\IA{marks}{mark}
\IR{mark}marks
\IA{non-standard edit}{non-standard}
\IR{non-standard}non-standard editing
\IA{read only files}{read only}
\IR{read only}read only files
\IA{selection}{selecting}
\IR{selection}selections
\IA{soft wrap}{softwrap}
\IA{spool files}{spool file}
\IR{spool file}spool files
\IA{font substyles}{sub-styles}
\IR{sub-styles}font substyles
\IA{word wrap}{wordwrap}
\IA{tabbing}{tabs}
\IR{anti-aliased}anti-aliased fonts

