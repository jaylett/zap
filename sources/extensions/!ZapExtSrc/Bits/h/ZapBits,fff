; Core text extension header

	GET	ZapSource:h.ZapHeader
	GET	<ZapExtSource$Dir>.h.RegNames
	GET	<ZapExtSource$Dir>.h.Macros

	MACRO
$m	Str	$token,$content
 [ INTL=""
$m	=	"$token"
 |
$m	=	"$content"
 ]
	MEND

	MACRO
$m	StrZ	$token,$content
 [ INTL=""
$m	=	"$token",0
 |
$m	=	"$content",0
 ]
	MEND

	MACRO
$m	StrZA	$token,$content
 [ INTL=""
$m	=	"$token",0
 |
$m	=	"$content",0
 ]
	ALIGN
	MEND

; Layout workspace requirements
; If you need workspace, add it here so that it will be claimed as part
; of the ZapBits module workspace.

		^	0
wk_textbuffer	#	256
wk_textlen	#	4


WorkspaceSize	#	0

	END
