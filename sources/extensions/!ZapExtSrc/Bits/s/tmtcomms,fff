	MOV	r2,#10
	BL	wipewithspaces
	FNcall	Zap_GetSel
	MOVCS	r1,#0
	MVNCC	r1,#0
	BL	insertasnumber
	FNRTS

selishere
	FNJSR	"r0-r2,r7-r9"
	MOV	r7,r8
	MOV	r2,#6
	BL	wipewithspaces
	FNcall	Zap_GetSel
	MVNCS	r7,#0
	CMP	r7,r8
	MOVNE	r1,#0
	MVNEQ	r1,#0
	BL	insertasnumber
	FNRTS

selhowbig
