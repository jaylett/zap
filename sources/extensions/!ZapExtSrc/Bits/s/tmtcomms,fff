	DCD	7+(3<<3)+(1<<6)
tmt_commandib
	FNJSR
	CMP	r2,#6
	BEQ	ibcprompt
	MOV	r8,#0
	MOV	r9,#0
	MOV	r10,#0
	MOV	R1,#1<<31				; don't learn
	FNcall	Zap_CommandString
	FNRTS
ibcprompt
	ADR	r0,ibcmdprompt
 [ INTL=""
	FNcall	Zap_MiniPromptToken, R2
 |
	FNcall	Zap_MiniPrompt, R2
 ]
 [ HIST=""
	MOVVC	r0,#lineed_buf_zapcommand
	FNcallc	Zap_SelectHistoryBuffer, VC, R2
 ]
	FNRTS

