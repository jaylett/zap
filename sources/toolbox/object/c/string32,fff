//
// control terminated string utilities
//
// $Id: string32,fff,v 1.1 2005/04/28 19:08:52 christian Exp $
//

#include <stdio.h>
#include "module.h"


char *string_cpy(char *dest, const char *s)
{
    char *d = dest;

    if (s == NULL) s = "";

    while (*s > 31)
        *d++ = *s++;

    *d = '\0';

    return dest;
}


int string_len(const char *s)
{
    int ret = 0;

    if (s == NULL) s = "";

    while (*s++ > 31)
        ret++;

    return ret;
}

