	BLT	RtAtStrtB	;  REM go round loop...
	LDREQB	r0, [r7]	;  REM      ! Tif's fix for lines that are
	TEQEQ	r0, #0x0A	;  REM      ! the same length as the width
	BEQ	haveajollygoodtime
haveajollygoodback
