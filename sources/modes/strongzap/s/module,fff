CurlyBrack
		;   "No {} NUFFIN"
	CMP	r6, #0
	ANDGT	r1, r2, #col_numbers
	STRGTB	r1, [r6, r3]
	BIC	r2, r2, #0xFF
	MOV	r1, r2, LSR #24
	AND	r1, r1, #0x01	;   Bits for {} depth
	CMP	r1, #0x00
	ORREQ	r2, r2, #col_body			; COL Cream/Normal
	ORRNE	r2, r2, #col_curlies
	B	AfterColour

