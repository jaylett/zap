	AND	r1, r2, #0xFF
	TEQ	r1, #col_link
	TEQNE	r1, #col_hash
	BEQ	BeforeColour
		; CMP	  1,#&12
		; OrrEQ	  1,#&1000000
	BIC	r2, r2, #0x1000000
	MOV	r1, r2, LSR #24
	AND	r1, r1, #0x01	;   Bits for {} depth
	TEQ	r1, #0x0
	BICNE	r2, r2, #0xFF
	ORRNE	r2, r2, #col_curlies
	ORRNE	r12, r12, #0x0200
	BNE	BeforeColour
	BIC	r2, r2, #0xFF
	ORR	r2, r2, #col_curlies
	BIC	r12, r12, #0x0200
	B	CurlyBrack

