		;     TST     9,#(1 << 19)
		;     MOVNE   1,#&02 ; was &12
		; .everythingsfinerightnow
	LDR	r9, storedr6flu
	CMP	r9, #0
	BLE	notANYone
	ANDS	r9, r2, #0xFF
	CMP	r9, #col_numbers
	CMPNE	r9, #col_strings
	BEQ	notANYone
	ADD	r9, r6, r3
	LDRB	r9, [r9, r11]		;  REM QUICK FIX...
	CMP	r9, #col_strings	;
	LDMEQFD	sp!, {r4-r12, pc}
	MOV	r9, #0
	ADD	r7, r6, r11
		; ADD	  7,7,#1 ; REM ??? TT
		; SUB	  8,8,#1
	ADD	r6, r7, r3
	LDRB	r5, [r6, r9]
	CMP	r5, #col_strings
	LDMEQFD	sp!, {r4-r12, pc}
	LDR	r11, adrofrsix
	ADRL	r5, w_formmb
	LDR	r5, [r5, #0x08]
lasdwA
	ADD	r4, r7, r9
	CMP	r4, r11
	BCC	beforesolhereargh
	LDRB	lr, [r7, r9]
	LDRB	r4, [r12, r9]
		; TST	  5,#1 << 27
