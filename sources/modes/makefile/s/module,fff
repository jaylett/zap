gotexactmatch
	ORR	r2, r2, #0x40000000	;  a command yet flags...
	ORR	r2, r2, #0x10000000	;  START OF LINE FLAG...
	BL	CheckRoutine
		;     TEQ     R8,R10
	TEQ	r9, #com_cfile
	TEQNE	r9, #com_header
	TEQNE	r9, #com_object
	TEQNE	r9, #com_assembler
	BEQ	Prefixbit
		;     TEQ     R8,R10
		;     TEQEQ   R9,#ASC"—"
		;     BEQ     Prefixbit2A
	;RSBEQ	r10, r11, #0 		;...pardon?
	;TEQ	r9, #156                ; this character isn't in the table
	;LDMEQFD	sp!, {r4-r12, pc}
	LDR	r1, beginnchar
		;     TEQ     1,#ASC"-"
		;     TEQNE   1,#ASC"!"
